{
  "name": "app-boilerplate",
  "description": "An Ionic project",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run post-build",
    "build:qa": "vue-cli-service build --dest dist-qa --mode staging && npm run post-build",
    "post-build": "npx cap copy & npx cap sync",
    "resources:android": "cordova-res android --skip-config --copy",
    "resources:ios": "cordova-res ios --skip-config --copy",
    "open:android": "npx cap open android",
    "run:android": "npx cap run android -l --external"
  },
  "dependencies": {
    "@capacitor/android": "^3.9.0",
    "@capacitor/core": "^3.2.0",
    "@capacitor/ios": "^3.2.0",
    "@ionic-native/core": "^5.36.0",
    "@ionic/vue": "^5.7.0",
    "@ionic/vue-router": "^5.7.0",
    "@vue/reactivity": "^3.2.6",
    "axios": "^0.21.1",
    "core-js": "^3.16.4",
    "ionicons": "^5.5.3",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.4.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "^2.0.0-rc.13",
    "cordova-res": "^0.15.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^7.17.0"
  }
}
